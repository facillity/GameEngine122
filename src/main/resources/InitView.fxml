<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="inf122.gui.SavageController">
  <children>
    <MenuBar prefHeight="29.0" prefWidth="555.0" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
            <MenuItem mnemonicParsing="false" text="Close" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About GameEngine" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane fx:id="btnslist" maxHeight="-1.0" maxWidth="-1.0" prefHeight="371.0" prefWidth="532.0" VBox.vgrow="ALWAYS">
         <children>
            <Text fill="#ff614a" layoutX="69.0" layoutY="31.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome! Please choose a game to play." textAlignment="CENTER">
               <font>
                  <Font name="Courier" size="22.0" />
               </font>
            </Text>
            <Button fx:id="tictactoe" layoutX="274.0" layoutY="48.0" mnemonicParsing="false" onAction="#launchGame" prefHeight="149.0" prefWidth="155.0" style="-fx-background-color: #ff6e49;" text="Tic-Tac-Toe">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <font>
                  <Font name="Courier Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="othello" layoutX="441.0" layoutY="48.0" mnemonicParsing="false" onAction="#launchGame" prefHeight="149.0" prefWidth="155.0" style="-fx-background-color: #ff6e49;" text="Othello">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <font>
                  <Font name="Courier Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="battleship" layoutX="274.0" layoutY="208.0" mnemonicParsing="false" onAction="#launchGame" prefHeight="149.0" prefWidth="155.0" style="-fx-background-color: #ff6e49;" text="Battleship">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <font>
                  <Font name="Courier Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="checkers" layoutX="441.0" layoutY="208.0" mnemonicParsing="false" onAction="#launchGame" prefHeight="149.0" prefWidth="155.0" style="-fx-background-color: #ff6e49;" text="Checkers">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <font>
                  <Font name="Courier Bold" size="18.0" />
               </font>
            </Button>
            <Text layoutX="24.0" layoutY="85.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Player 1 Name:">
               <font>
                  <Font name="Courier" size="15.0" />
               </font>
            </Text>
            <Text layoutX="24.0" layoutY="159.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Player 2 Name:">
               <font>
                  <Font name="Courier" size="15.0" />
               </font>
            </Text>
            <TextField fx:id="player1Name" onAction="#enterPlayer1Name" layoutX="24.0" layoutY="96.0" prefHeight="27.0" prefWidth="213.0" />
            <TextField fx:id="player2Name" onAction="#enterPlayer2Name" layoutX="25.0" layoutY="170.0" prefHeight="27.0" prefWidth="213.0" />
         </children>
    </AnchorPane>
  </children>
</VBox>
